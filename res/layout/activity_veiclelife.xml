<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
<RelativeLayout 
        android:id="@+id/rel_link"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/orangelight">

        <LinearLayout
            android:id="@+id/lin_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/img_back"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/back" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/carlife"
                android:textColor="@color/white"
                android:textSize="20sp" />
        </LinearLayout>

    </RelativeLayout>
    
<LinearLayout 
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:orientation="vertical"
    >
    <RelativeLayout
     android:layout_marginTop="10dp"
     android:layout_marginLeft="10dp"
     android:id="@+id/rel_baoyangdata"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
        >

     <TextView
         android:id="@+id/tv_baoyangdataotv_baoyangdata"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:textSize="16dp"
         android:text="保养日期" />
      <TextView 
          android:id="@+id/tv_baoyangdata"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignParentRight="true"
         android:layout_marginRight="10dp"
          android:textSize="16dp"
         android:text=""
          />     
    </RelativeLayout>
   <View 
       android:layout_width="match_parent"
       android:layout_height="1dp"
       android:layout_marginLeft="10dp"
       android:layout_marginRight="10dp"
       android:layout_marginTop="5dp"
       android:background="@color/textgraylight"      
       /> 
          <RelativeLayout 
  
     android:layout_marginTop="10dp"
     android:layout_marginLeft="10dp"
     android:id="@+id/rel_baoxianriqi"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
        >

     <TextView
         android:id="@+id/tv_baoxianriqio"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:textSize="16dp"
         android:text="保险日期" />
      <TextView 
          android:id="@+id/tv_baoxianriqi"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignParentRight="true"
         android:layout_marginRight="10dp"
          android:textSize="16dp"
         android:text=""
          />     
    </RelativeLayout>
     <View 
       android:layout_width="match_parent"
       android:layout_height="1dp"
       android:layout_marginLeft="10dp"
       android:layout_marginRight="10dp"
       android:layout_marginTop="5dp"
       android:background="@color/textgraylight"      
       />
     
               <RelativeLayout 
     android:layout_marginTop="10dp"
     android:layout_marginLeft="10dp"
     android:id="@+id/rel_nianjiandata"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
        >

     <TextView
         android:id="@+id/tv_nianjiandatao"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:textSize="16dp"
         android:text="年检日期" />
      <TextView 
          android:id="@+id/tv_nianjiandata"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignParentRight="true"
         android:layout_marginRight="10dp"
          android:textSize="16dp"
         android:text=""
          />     
    </RelativeLayout>
    <View 
       android:layout_width="match_parent"
       android:layout_height="1dp"
       android:layout_marginLeft="10dp"
       android:layout_marginRight="10dp"
       android:layout_marginTop="5dp"
       android:background="@color/textgraylight"      
       />
        
     <RelativeLayout 
     android:layout_marginTop="10dp"
     android:layout_marginLeft="10dp"
     android:id="@+id/rel_weizhangdata"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
        >

     <TextView
         android:id="@+id/tv_weizhangdatao"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:textSize="16dp"
         android:text="违章信息" />
      <TextView 
          android:id="@+id/tv_weizhangdata"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignParentRight="true"
         android:layout_marginRight="10dp"
          android:textSize="16dp"
       
          />     
    </RelativeLayout>
    <View 
       android:layout_width="match_parent"
       android:layout_height="1dp"
       android:layout_marginLeft="10dp"
       android:layout_marginRight="10dp"
       android:layout_marginTop="5dp"
       android:background="@color/textgraylight"      
       />
        
</LinearLayout>
</LinearLayout>
