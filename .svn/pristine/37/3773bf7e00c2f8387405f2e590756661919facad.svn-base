package com.Entity;

import java.util.ArrayList;
import java.util.List;

public class CarReportBriefSet {
	private List<CarReportBrief> BriefList = new ArrayList<CarReportBrief>();
	public CarReportBriefSet(List<CarReportBrief> List){
		this.BriefList = List;
	}
	
	public int getSize(){
		return BriefList.size();
	}


	public CarReportBrief getItem(int arg0){
		return BriefList.get(arg0);
	}

	public List<CarReportBrief> getBriefList() {
		return BriefList;
	}

	public void setBriefList(List<CarReportBrief> briefList) {
		BriefList = briefList;
	}
	
	
}
