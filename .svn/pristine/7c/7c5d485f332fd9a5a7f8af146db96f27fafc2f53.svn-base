package com.Entity;

import java.util.ArrayList;
import java.util.List;

public class InsuranceRecordSet {


private List<InsuranceRecord> InsuranceRecordList = new ArrayList<InsuranceRecord>();
	
	public InsuranceRecordSet(List<InsuranceRecord> InsuranceRecordlist){
		this.InsuranceRecordList = InsuranceRecordlist;
	}
	
	
	public InsuranceRecordSet() {
		// TODO Auto-generated constructor stub
	}


	public List<InsuranceRecord> getInsuranceRecordList() {
		return InsuranceRecordList;
	}


	public void setInsuranceRecordList(List<InsuranceRecord> insuranceRecordList) {
		InsuranceRecordList = insuranceRecordList;
	}


	public int getSize(){
		return InsuranceRecordList.size();
	}


	public InsuranceRecord getItem(int arg0){
		return InsuranceRecordList.get(arg0);
	}
}
