package com.Entity;

import java.util.ArrayList;
import java.util.List;

public class DataFlowSet {
private List<DataFlow> DataFlowList = new ArrayList<DataFlow>();
	
	public DataFlowSet(List<DataFlow> DataFlowlist){
		this.DataFlowList = DataFlowlist;
	}
	
	
	public int getSize(){
		return DataFlowList.size();
	}


	public DataFlow getItem(int arg0){
		return DataFlowList.get(arg0);
	}
}
