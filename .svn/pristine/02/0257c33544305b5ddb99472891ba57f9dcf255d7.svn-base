package com.Entity;

public class Red {

	private int red_id;		 		//红包id
	private int red_type;  			//红包类型（0-消费  1-获取）
	private int red_num;			//红包数目
	private int way_type;			//红包途径
	private String update_time;		//更新时间
	private String way_name;		//途径名称
	
	
	public int getRed_id() {
		return red_id;
	}
	public void setRed_id(int red_id) {
		this.red_id = red_id;
	}
	public int getRed_type() {
		return red_type;
	}
	public void setRed_type(int red_type) {
		this.red_type = red_type;
	}
	public int getRed_num() {
		return red_num;
	}
	public void setRed_num(int red_num) {
		this.red_num = red_num;
	}
	public int getWay_type() {
		return way_type;
	}
	public void setWay_type(int way_type) {
		this.way_type = way_type;
	}
	public String getUpdate_time() {
		return update_time;
	}
	public void setUpdate_time(String update_time) {
		this.update_time = update_time;
	}
	public String getWay_name() {
		return way_name;
	}
	public void setWay_name(String way_name) {
		this.way_name = way_name;
	}
	
	
	
	
}
