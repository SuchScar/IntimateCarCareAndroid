package com.Entity;

public class Preferential {

	private int preferential_id;		//优惠id
	private int type;					//类型
	private float discount;				//折扣
	private float preferential_money;	//优惠价格
	private float full_money;			//消费满多少
	private float reduce_money;			//减多少
	private String start_time;			//开始时间
	private String end_time;			//结束时间
	public int getPreferential_id() {
		return preferential_id;
	}
	public void setPreferential_id(int preferential_id) {
		this.preferential_id = preferential_id;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public float getDiscount() {
		return discount;
	}
	public void setDiscount(float discount) {
		this.discount = discount;
	}
	public float getPreferential_money() {
		return preferential_money;
	}
	public void setPreferential_money(float preferential_money) {
		this.preferential_money = preferential_money;
	}
	public float getFull_money() {
		return full_money;
	}
	public void setFull_money(float full_money) {
		this.full_money = full_money;
	}
	public float getReduce_money() {
		return reduce_money;
	}
	public void setReduce_money(float reduce_money) {
		this.reduce_money = reduce_money;
	}
	public String getStart_time() {
		return start_time;
	}
	public void setStart_time(String start_time) {
		this.start_time = start_time;
	}
	public String getEnd_time() {
		return end_time;
	}
	public void setEnd_time(String end_time) {
		this.end_time = end_time;
	}
	
	
	
	
	
	
}
