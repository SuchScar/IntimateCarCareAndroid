package com.Entity;

import java.util.ArrayList;
import java.util.List;

public class MaintainItemSet {
private List<MaintainItem>MaintainItemList = new ArrayList<MaintainItem>();
	
	public MaintainItemSet(List<MaintainItem> MaintainItemlist){
		this.MaintainItemList = MaintainItemlist;
	}
	
	
	public int getSize(){
		return MaintainItemList.size();
	}
	


	public List<MaintainItem> getMaintainItemList() {
		return MaintainItemList;
	}


	public void setMaintainItemList(List<MaintainItem> maintainItemList) {
		MaintainItemList = maintainItemList;
	}


	public MaintainItem getItem(int arg0){
		return MaintainItemList.get(arg0);
	}
}
