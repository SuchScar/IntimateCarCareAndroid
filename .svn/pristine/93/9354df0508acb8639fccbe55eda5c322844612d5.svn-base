public class AnnualInspectionSet {
private List<AnnualInspection> AnnualInspectionList = new ArrayList<AnnualInspection>();
	
	public AnnualInspectionSet(List<AnnualInspection> AnnualInspectionlist){
		this.AnnualInspectionList = AnnualInspectionlist;
	}
	
	public List<AnnualInspection> getAnnualInspectionList() {
		return AnnualInspectionList;
	}
	public int getSize(){
