package com.Entity;

import java.util.ArrayList;
import java.util.List;

public class RedSet {
private List<Red> RedList = new ArrayList<Red>();
	
	public RedSet(List<Red> Redlist){
		this.RedList = Redlist;
	}
	
	
	public int getSize(){
		return RedList.size();
	}


	public List<Red> getRedList() {
		return RedList;
	}


	public void setRedList(List<Red> redList) {
		RedList = redList;
	}


	public Red getItem(int arg0){
		return RedList.get(arg0);
	}
}
